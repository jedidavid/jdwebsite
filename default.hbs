<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>Jeremy David</title>
  <link rel="stylesheet" type="text/css" href="assets/css/screen.css" />
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/css/materialize.min.css">
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <link rel="stylesheet" href="assets/css/header.css">
  <link href="https://fonts.googleapis.com/css?family=Raleway" rel="stylesheet">
  {{ghost_head}}
</head>

<body>
  {{{body}}}
  <script src="http://code.jquery.com/jquery-3.3.1.min.js" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8="
    crossorigin="anonymous"></script>
  <script>
    // Scrolling effect
    $(document).ready(function () {
      // Add smooth scrolling to all links
      

      $(".button-collapse").sideNav({ //SIDENAV
        menuWidth: 300, // Default is 300
        edge: 'left', // Choose the horizontal origin
        closeOnClick: true
      });

      $('.parallax').parallax(); //PARALLAX
      $('.modal').modal(); //MODAL
      $('.materialboxed').materialbox(); //MATERIAL BOX
      $('.scrollspy').scrollSpy(); //SCROLL SPY


      // START SCROLLFIRE
      var options = [
        {selector: '.navbar-fixed', offset: 1300, callback: function () {
          $('nav').removeClass('transparent');
          $('nav').addClass('white');
          $('.navlinks').addClass('black-text');



        }},
        {selector: '.main-text', offset: 0, callback: function (el) {
          Materialize.fadeInImage($(el));
        }},
        {selector: '#about', offset: 300, callback: function (el) {
          Materialize.fadeInImage($(el));
        }},
        {selector: '#portfolio', offset: 300, callback: function (el) {
          Materialize.fadeInImage($(el));
        }},
        {selector: '#skills', offset: 300, callback: function (el) {
          Materialize.fadeInImage($(el));
        }},
        {selector: '#frameworks', offset: 300, callback: function (el) {
          Materialize.fadeInImage($(el));
        }},
        {selector: '#oop', offset: 300, callback: function (el) {
          Materialize.fadeInImage($(el));
        }},
        {selector: '#database', offset: 300, callback: function (el) {
          Materialize.fadeInImage($(el));
        }},
        {selector: '#os', offset: 300, callback: function (el) {
          Materialize.fadeInImage($(el));
        }},


      ];
        Materialize.scrollFire(options);
      // END SCROLLFIRE

      
    });


    
    

  


    
  </script>

  <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/js/materialize.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/particles.js@2.0.0/particles.min.js"></script>
  <script>
    particlesJS('particles-js',

      {
        "particles": {
          "number": {
            "value": 120,
            "density": {
              "enable": true,
              "value_area": 800
            }
          },
          "color": {
            "value": "#66cd1e"
          },
          "shape": {
            "type": "circle",
            "stroke": {
              "width": 0,
              "color": "#ffffff"
            },
            "polygon": {
              "nb_sides": 5
            },
            "image": {
              "src": "img/github.svg",
              "width": 100,
              "height": 100
            }
          },
          "opacity": {
            "value": 0.5,
            "random": false,
            "anim": {
              "enable": false,
              "speed": 1,
              "opacity_min": 0.1,
              "sync": false
            }
          },
          "size": {
            "value": 2,
            "random": true,
            "anim": {
              "enable": false,
              "speed": 40,
              "size_min": 0.1,
              "sync": false
            }

          },
          "line_linked": {
            "enable": true,
            "distance": 150,
            "color": "#ffffff",
            "opacity": 0.4,
            "width": 1
          },
          "move": {
            "enable": true,
            "speed": 2,
            "direction": "none",
            "random": false,
            "straight": false,
            "out_mode": "out",
            "bounce": false,
            "attract": {
              "enable": false,
              "rotateX": 600,
              "rotateY": 1200
            }
          }
        },
        "interactivity": {
          "detect_on": "canvas",
          "events": {
            "onhover": {
              "enable": false,
              "mode": "repulse"
            },
            "onclick": {
              "enable": false,
              "mode": "push"
            },
            "resize": true
          },
          "modes": {
            "grab": {
              "distance": 400,
              "line_linked": {
                "opacity": 1
              }
            },
            "bubble": {
              "distance": 400,
              "size": 40,
              "duration": 2,
              "opacity": 8,
              "speed": 3
            },
            "repulse": {
              "distance": 200,
              "duration": 0.4
            },
            "push": {
              "particles_nb": 4
            },
            "remove": {
              "particles_nb": 2
            }
          }
        },
        "retina_detect": true
      }

    );
  </script>

</body>

{{> footer}} 
{{ghost_foot}}

</html>